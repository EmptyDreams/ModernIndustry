{
  "group": "synthesizer",
  "output": "*",
  "input": [
    "#$$$#",
    "#---#",
    "#   #",
    "#===#",
    "#%=%#"
  ],
  "key": {
    "#": { "item": "mi:machine_shell" },
    "$": { "item": "mi:coil" },
    "-": { "item": "minecraft:piston" },
    "=": { "item": "mi:iron_plate" },
    "%": { "item": "mi:circuit" },
    "*": { "item": "mi:compressor" }
  }
}